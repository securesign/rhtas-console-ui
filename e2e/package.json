{
  "name": "@console-ui/e2e",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./test-results ./playwright-report",
    "clean:all": "rimraf ./dist ./node_modules",
    "codegen": "npx playwright codegen http://localhost:3000",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check './tests/**/*.{ts,tsx,js,json}'",
    "format:fix": "prettier --write './tests/**/*.{ts,tsx,js,json}'",
    "test": "npx playwright test --project='chromium'",
    "test:trace": "npx playwright test --project='chromium' --trace on",
    "test:host": "npx playwright test --ui-host 127.0.0.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.57.0"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.11.0"
  }
}
